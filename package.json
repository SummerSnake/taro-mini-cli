{
	"name": "taro-mini-cli",
	"version": "1.0.0",
	"private": true,
	"description": "taro 小程序脚手架",
	"templateInfo": {
		"name": "default",
		"typescript": false,
		"css": "sass"
	},
	"scripts": {
		"dev": "cross-env ENV=dev taro build --type weapp --watch",
		"start": "cross-env ENV=dev taro build --type weapp --watch",
		"compress": "cross-env ENV=dev NODE_ENV=production taro build --type weapp --watch",
		"test": "cross-env ENV=test taro build --type weapp",
		"uat": "cross-env ENV=uat taro build --type weapp",
		"build": "cross-env ENV=prod taro build --type weapp",
		"template": "node scripts/template",
		"prettier": "node scripts/prettier.js"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "SummerSnake",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "7.18.0",
		"@tarojs/components": "3.4.9",
		"@tarojs/react": "3.4.9",
		"@tarojs/runtime": "3.4.9",
		"@tarojs/taro": "3.4.9",
		"dva-core": "2.0.2",
		"dva-loading": "3.0.20",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-redux": "8.0.1",
		"redux": "4.2.0",
		"taro-ui": "3.0.0-alpha.10"
	},
	"devDependencies": {
		"@babel/core": "7.18.0",
		"@tarojs/cli": "3.4.9",
		"@tarojs/mini-runner": "3.4.9",
		"@tarojs/plugin-framework-react": "3.4.9",
		"@tarojs/webpack-runner": "3.4.9",
		"@types/react": "17.0.2",
		"@types/webpack-env": "1.17.0",
		"babel-preset-taro": "3.4.9",
		"cross-env": "7.0.2",
		"eslint": "8.16.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-taro": "3.4.9",
		"eslint-plugin-import": "2.26.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.30.0",
		"eslint-plugin-react-hooks": "4.5.0",
		"prettier": "2.6.2",
		"sass": "1.52.1"
	}
}
