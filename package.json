{
	"name": "taro-mini-cli",
	"version": "1.0.0",
	"private": true,
	"description": "taro 小程序脚手架",
	"templateInfo": {
		"name": "default",
		"typescript": false,
		"css": "sass"
	},
	"scripts": {
		"dev": "cross-env ENV=dev taro build --type weapp --watch",
		"start": "cross-env ENV=dev taro build --type weapp --watch",
		"compress": "cross-env ENV=dev NODE_ENV=production taro build --type weapp --watch",
		"test": "cross-env ENV=test taro build --type weapp",
		"uat": "cross-env ENV=uat taro build --type weapp",
		"build": "cross-env ENV=prod taro build --type weapp",
		"template": "node scripts/template",
		"prettier": "node scripts/prettier.js"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "SummerSnake",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "7.20.1",
		"@tarojs/components": "3.5.7",
    "@tarojs/helper": "3.5.7",
    "@tarojs/plugin-framework-react": "3.5.7",
    "@tarojs/plugin-platform-weapp": "3.5.7",
    "@tarojs/react": "3.5.7",
    "@tarojs/runtime": "3.5.7",
    "@tarojs/shared": "3.5.7",
    "@tarojs/taro": "3.5.7",
		"react": "18.2.0",
    "react-dom": "18.2.0",
		"taro-ui": "3.1.0-beta.4"
	},
	"devDependencies": {
    "@babel/core": "7.20.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.9",
    "@tarojs/cli": "3.5.7",
    "@tarojs/mini-runner": "3.5.7",
    "@tarojs/webpack5-runner": "3.5.7",
    "@types/react": "18.0.25",
    "@types/webpack-env": "1.18.0",
    "babel-preset-taro": "3.5.7",
		"cross-env": "7.0.2",
    "eslint": "8.27.0",
    "eslint-config-taro": "3.5.7",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.7.1",
    "react-refresh": "0.14.0",
    "sass": "1.56.1"
	}
}
